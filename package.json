{
  "name": "moleculer-gundb",
  "version": "0.1.0",
  "description": "GunDB Gateway for MoleculerJS",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "dev": "npm run build && nodemon lib/examples/index.js",
    "devc": "npm run build:dev | nodemon lib/examples/index.js",
    "ci": "jest --watch --coverage",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "deploy": "npm version patch && npm run build && npm run deploy:lib",
    "deploy:lib": "npm publish ./ --access public"
  },
  "keywords": [
    "moleculer",
    "microservice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smart-matrix/moleculer-gundb.git"
  },
  "author": "Fathym, Inc",
  "license": "MIT",
  "peerDependencies": {
    "moleculer": "^0.13.0",
    "gun": "^0.9.9992"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.10",
    "benchmarkify": "2.1.0",
    "coveralls": "3.0.2",
    "eslint": "5.9.0",
    "jest": "^23.6.0",
    "jest-cli": "23.6.0",
    "moleculer": "^0.13.0",
    "nodemon": "1.18.6",
    "npm-check": "5.9.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "jester": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js)$"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "dependencies": {
    "gun": "^0.9.9992"
  }
}
